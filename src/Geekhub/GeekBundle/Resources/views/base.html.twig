<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}Welcome!{% endblock %}</title>

    <meta charset="UTF-8"/>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% block stylesheets %}
        <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/geekhubgeek/css/style.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/geekhubgeek/css/bootstrap.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/geekhubgeek/css/bootstrap-theme.css') }}"/>
    {% endblock %}

    {% block javascript %}
        <script src="{{ asset('bundles/geekhubgeek/js/jquery-1.11.1.min.js') }}"></script>
        <script src="{{ asset('bundles/geekhubgeek/js/bootstrap.js') }}"></script>
    {% endblock %}
</head>
<body>
{% block body %}
    <div class="page-header">
        <div id="header-container">
            {% block content_header %}
            {% endblock %}
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="block container">
        {% block content %}{% endblock %}
    </div>
{% endblock %}
</body>
</html>
